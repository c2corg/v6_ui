{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "extends": [
    "eslint:recommended",
    "openlayers"
  ],
  "globals": {
    "DEBUG": false,
    "$": false,
    "Bloodhound": false,
    "moment": false
  },
  "rules": {
    "no-console": 1,
    "valid-jsdoc": 0,
    "prefer-arrow-callback": 2,
    "no-var": 2,
    "no-multi-assign": 2
  }
}
